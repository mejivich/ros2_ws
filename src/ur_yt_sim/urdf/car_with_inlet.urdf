<?xml version="1.0"?>
<robot name="car_with_inlet">

  <!-- =========================
       INLET
       ========================= -->
  <link name="inlet">
    <visual>
      <geometry>
        <mesh filename="package://inlet/meshes/inlet.dae"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.15 0.08 0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4"/>
      <inertia ixx="0.0004" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0003"/>
    </inertial>
  </link>

  <joint name="inlet_joint" type="fixed">
    <parent link="chassis"/>
    <child link="inlet"/>
    <origin xyz="0.480613 0.757583 0.40198" rpy="0 0 1.52002"/>
  </joint>

  <!-- =========================
       CHASSIS
       ========================= -->
  <link name="chassis">
    <visual>
      <geometry>
        <mesh filename="package://prius_hybrid/meshes/Hybrid.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="1.7526 2.1 0.95"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1326"/>
      <inertia ixx="2581.13" ixy="0" ixz="0" iyy="591.308" iyz="0" izz="2681.95"/>
      <origin xyz="0 -0.266 0.48" rpy="0 0 0"/>
    </inertial>
  </link>

  <!-- =========================
       STEERING WHEEL
       ========================= -->
  <link name="steering_wheel">
    <visual>
      <geometry>
        <mesh filename="package://prius_hybrid/meshes/Hybrid.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.178172" length="0.041845"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
    </inertial>
  </link>

  <joint name="steering_joint" type="revolute">
    <parent link="chassis"/>
    <child link="steering_wheel"/>
    <origin xyz="-0.002 0 0" rpy="0 0 0"/>
    <axis xyz="0 0.964095 0.265556"/>
    <limit lower="-7.85" upper="7.85"/>
  </joint>

  <!-- =========================
       WHEELS
       ========================= -->
  <!-- Front Left -->
  <link name="front_left_wheel">
    <visual>
      <geometry>
        <mesh filename="package://prius_hybrid/meshes/Hybrid.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.31265"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="11"/>
      <inertia ixx="0.586312" ixy="0" ixz="0" iyy="0.335529" iyz="0" izz="0.335529"/>
    </inertial>
  </link>

  <joint name="front_left_combined_joint" type="universal">
    <parent link="chassis"/>
    <child link="front_left_wheel"/>
    <origin xyz="0 0 0" rpy="-0.087266 0 0"/>
    <axis xyz="0 0 1">
      <limit lower="-0.8727" upper="0.8727"/>
    </axis>
    <axis xyz="1 0 0"/>
  </joint>

  <!-- Front Right -->
  <link name="front_right_wheel">
    <visual>
      <geometry>
        <mesh filename="package://prius_hybrid/meshes/Hybrid.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.31265"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="11"/>
      <inertia ixx="0.586312" ixy="0" ixz="0" iyy="0.335529" iyz="0" izz="0.335529"/>
    </inertial>
  </link>

  <joint name="front_right_combined_joint" type="universal">
    <parent link="chassis"/>
    <child link="front_right_wheel"/>
    <origin xyz="0 0 0" rpy="-0.087266 0 0"/>
    <axis xyz="0 0 1">
      <limit lower="-0.8727" upper="0.8727"/>
    </axis>
    <axis xyz="1 0 0"/>
  </joint>

  <!-- =========================
       REAR AXLE AND WHEELS
       ========================= -->
  <link name="rear_axle">
    <visual>
      <geometry>
        <cylinder radius="0.075" length="1.357"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="30"/>
      <inertia ixx="0.084375" ixy="0" ixz="0" iyy="4.64581" iyz="0" izz="4.64581"/>
    </inertial>
  </link>

  <joint name="rear_axle_joint" type="revolute">
    <parent link="chassis"/>
    <child link="rear_axle"/>
    <axis xyz="0 1 0">
      <limit lower="-0.0508906" upper="0.0508906"/>
    </axis>
  </joint>

  <link name="rear_left_wheel">
    <visual>
      <geometry>
        <mesh filename="package://prius_hybrid/meshes/Hybrid.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.31265"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="11"/>
      <inertia ixx="0.586312" ixy="0" ixz="0" iyy="0.335529" iyz="0" izz="0.335529"/>
    </inertial>
  </link>

  <joint name="rear_left_wheel_joint" type="revolute">
    <parent link="rear_axle"/>
    <child link="rear_left_wheel"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name="rear_right_wheel">
    <visual>
      <geometry>
        <mesh filename="package://prius_hybrid/meshes/Hybrid.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.31265"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="11"/>
      <inertia ixx="0.586312" ixy="0" ixz="0" iyy="0.335529" iyz="0" izz="0.335529"/>
    </inertial>
  </link>

  <joint name="rear_right_wheel_joint" type="revolute">
    <parent link="rear_axle"/>
    <child link="rear_right_wheel"/>
    <axis xyz="1 0 0"/>
  </joint>

</robot>
